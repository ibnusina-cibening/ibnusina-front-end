{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6","dom", "dom.iterable", "esnext"], // es6 bawaan mui 
    "noImplicitAny": true, // bawaan mui
    "noImplicitThis": true, // bawaan mui
    "strictNullChecks": true, // bawaan mui
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "exclude": ["node_modules"],
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"]
}
